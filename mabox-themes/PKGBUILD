pkgname=mabox-themes
pkgver=20160325
pkgrel=1
pkgdesc="Themes for for Mabox"
arch=(any)
url="https://github.com/MaboxOS/$pkgname"
license=(GPL)
depends=()
makedepends=('git')
source=("git+$url.git")
md5sums=('SKIP')

package () {
	install -dm755 $pkgdir/usr/share/themes
	rm -r $pkgname/.git
	cp -r $pkgname/* $pkgdir/usr/share/themes/
	
	
	
}
